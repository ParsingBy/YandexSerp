{
    "name": "parsingby/yandexserp",
    "description": "Парсер выдачи Яндекса",
    "keywords": [
        "parsingby",
        "yandexserp"
    ],
    "homepage": "https://github.com/parsingby/yandexserp",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Unknown",
            "email": "unknown@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ParsingBy\\YandexSerp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ParsingBy\\YandexSerp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ParsingBy\\YandexSerp\\YandexSerpServiceProvider"
            ],
            "aliases": {
                "YandexSerp": "ParsingBy\\YandexSerp\\YandexSerpFacade"
            }
        }
    }
}
